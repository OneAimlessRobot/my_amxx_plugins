			set_task(0.1, "task_respawn", TASKID_RESPAWN + lucky_bastard)
		}
		else
			failed_revive(id)
	}
	else
		set_task(0.1, "task_revive", TASKID_REVIVE + id)
	
	return FMRES_IGNORED
}

