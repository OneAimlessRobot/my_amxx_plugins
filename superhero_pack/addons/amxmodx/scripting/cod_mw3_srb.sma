public WybierzKlase_Handle(id, menu, item) 
{ 
	client_cmd(id, "spk mw/select"); 
	
	if(item == MENU_EXIT) 
	{ 
		menu_destroy(menu); 
		return PLUGIN_CONTINUE; 
	}        
	
	item++; 
	
	new ile = 0; 
	for(new i=1; i<sizeof nazwy_klas; i++) 
	{ 
		if(nalezy_do[i] == frakcja_gracza[id]){ 
